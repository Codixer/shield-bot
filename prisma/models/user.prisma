model User {
  id            Int              @id @default(autoincrement())
  discordId     String           @unique
  vrchatAccounts VRChatAccount[]
}

model VRChatAccount {
  id        Int    @id @default(autoincrement())
  user      User   @relation(fields: [userId], references: [id])
  userId    Int
  vrcUserId String
  accountType VRChatAccountType
}
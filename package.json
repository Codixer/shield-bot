{
  "name": "shield-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "prisma": {
    "schema": "./prisma"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "build:prod": "tsc -p ./tsconfig.json",
    "build:changelog": "npx @discordx/changelog --src src",
    "dev": "ts-node-esm src/main.ts",
    "start": "ENV=development node build/main.js",
    "start:prod": "node build/main.js",
    "watch": "nodemon --exec ts-node-esm src/main.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org jasmeow-systems --project jasmeow-systems-bot ./build && sentry-cli sourcemaps upload --org jasmeow-systems --project jasmeow-systems-bot ./build"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "@discordx/koa": "^1.2.2",
    "@discordx/pagination": "^3.5.6",
    "@discordx/utilities": "^7.0.3",
    "@koa/bodyparser": "^5.1.1",
    "@koa/multer": "^3.0.2",
    "@koa/router": "^13.1.0",
    "@prisma/client": "6.7.0",
    "axios": "^1.7.9",
    "discord.js": "^14.19.2",
    "discordx": "^11.12.4",
    "dotenv": "^16.4.7",
    "koa": "^3.0.0",
    "multer": "1.4.5-lts.2",
    "mysql": "^2.18.1",
    "node-cron": "^4.0.5",
    "otplib": "^12.0.1",
    "vrchat": "^1.19.1"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa__multer": "^2.0.7",
    "@types/koa__router": "^12.0.4",
    "@types/mysql": "^2.15.26",
    "@types/node": "^22.13.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.12",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "prisma": "6.7.0",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "yarn@1.22.22"
}
